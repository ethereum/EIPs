        $('.js-freemium-cta-dismiss').click(function (e) {
            e.preventDefault();
            StackExchange.helpers.toggleAccountPreferenceFlags(32, true);
            $('.js-freemium-cta').addClass('d-none');
            $('.js-create-team-cta').removeClass('d-none');
            return false;
        });
