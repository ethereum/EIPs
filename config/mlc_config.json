{
  "ignorePatterns": [
    // Escaping ']' to avoid malformed character class in regex; matches optional backticks before 'csl-json'
    "[`\]]*csl-json"
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "/github/workspace/"
    }
  ],
  "aliveStatusCodes": [
    200,
    429
  ]
}
